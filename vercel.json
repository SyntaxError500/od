{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["dist/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(css|js|png|jpg|svg|ico|woff|woff2)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
